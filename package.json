{
  "name": "Connect4",
  "version": "1.0.0",
  "repository": "https://github.com/Msatkin/Connect4",
  "description": "A HTML connect 4 game built using PixiJS",
  "homepage": "https://msatkin.github.io/Connect4",
  "config": {
    "buildDir": "./build"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm run build && webpack-dev-server --content-base ./build/ --hot --inline",
    "webpack": "webpack -p",
    "build": "npm run webpack"
  },
  "dependencies": {
    "pixi-display": "^1.1.1",
    "pixi.js": "^4.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.6.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.4.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
